    "next": "16.1.6",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.2.4",
    "react-image-file-resizer": "^0.4.8",
    "sharp": "^0.34.3",
    "styled-jsx": "^5.1.7",
    "yet-another-react-lightbox": "^3.29.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
